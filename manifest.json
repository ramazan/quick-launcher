{
  "manifest_version": 2,
  "name": "Quick Launcher",
  "description": "A chrome extension to quickly access sites/products/tools deployed in different environments(UAT, Staging, Production).",
  "version": "1.0.0",
  "icons": {
    "16": "static/img/icon_16x16.png",
    "48": "static/img/icon_48x48.png",
    "128": "static/img/icon_128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "static/img/icon_19x19.png",
      "38": "static/img/icon_38x38.png"
    },
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["lib/jquery-3.4.1.min.js", "static/js/script.js"]
  },
  "permissions": ["activeTab", "bookmarks", "storage"]
}
